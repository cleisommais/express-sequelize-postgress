{
  "name": "express-sequelize-postgress",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./postgres --copy-files",
    "debug": "set DEBUG=* & nodemon --exec babel-node ./bin/www.js",
    "dev": "nodemon --exec babel-node ./bin/www.js",
    "start": "node bin/www.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "dotenv": "^8.0.0",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3",
    "sequelize-cli": "^6.2.0"
  }
}
